<head>
  <meta charset="UTF-8"  />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>{{default title "Kobe"}}</title>
  <link href="/assets/styles.css" rel="stylesheet">
  <script src="/assets/alpine.js" defer=""></script>
  <link rel="shortcut icon" type="image/svg+xml" href="/assets/icon-512x512.png"/>
  <link rel="manifest" href="/manifest.json" />
  <script>
    if(false)
      navigator.serviceWorker.register("/serviceworker.js", { scope: "." }).then(console.log, console.error)
    if ("serviceWorker" in navigator) 
      navigator.serviceWorker.getRegistrations().then(registrations => { for (const registration of registrations) { registration.unregister(); } });
  </script>
  {{> @partial-block }}
</head>